<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jwt.hibernate.bean">
    <class name="RegisterDetailsVo.java" table="regtable">
        <id column="id" name="Id"  type="java.lng.Integer"></id>
        <property column="name" name="Name" type="java.lang.String"></property>
        <property column="password" name="Password" type="java.lang.String"></property>
    	<property column="address" name="Address" type="java.lang.String"></property>
    	<property column="email" name="Email" type="java.lang.String"></property>
    	<property column="phoneNumber" name="PhoneNumber" type="java.lang.String"></property>
    </class>
</hibernate-mapping>